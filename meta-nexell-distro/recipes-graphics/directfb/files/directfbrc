### system = /dev/fb0 ###
# desktop-buffer-mode
# "uto|triple|backvideo|backsystem|frontonly|windows"
# - backvideo = support double buffer for the fbdev
desktop-buffer-mode = backvideo

### system = drmkms
###
# allocated first in 'system memory'.
# must be disabled if set 'accelerator'
drmkms-surface-pool-priority-default

# drmkms-buffer-
# must be disabled if set 'accelerator'
# video        : memory = video , non-cachable
# video-cache  : memory = video , cachable
# system       : memory = system, non-cachable
# system-cache : memory = system, cachable
drmkms-buffer-system  = video-cache
drmkms-buffer-primary = video

